{
    "name": "@flyingboat/kofono-solid-ui",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "source": "./src/index.ts",
    "exports": {
        "solid": "./dist/index.jsx",
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": ["dist", "src"],
    "scripts": {
        "build": "pnpm build:ts && pnpm build:tsup",
        "build:ts": "tsc -p tsconfig.json",
        "build:tsup": "tsup --config tsup.config.ts",
        "build:watch": "tsup --config tsup.config.ts --watch",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "dev": "vite serve dev",
        "lint": "pnpm dlx @biomejs/biome check",
        "lint:fix": "pnpm lint --write",
        "format": "pnpm dlx @biomejs/biome format",
        "old:build:vite": "vite build",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "@flyingboat/kofono": "workspace:*",
        "@kobalte/core": "0.13.11",
        "@solid-primitives/i18n": "2.2.1",
        "@solid-primitives/scheduled": "1.5.2",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "solid-js": "^1.9.10"
    },
    "peerDependencies": {
        "solid-js": "^1.6.0",
        "tailwindcss": "^4.1.11"
    },
    "devDependencies": {
        "@solidjs/testing-library": "^0.8.10",
        "@tailwindcss/postcss": "^4.1.18",
        "@tailwindcss/vite": "^4.1.18",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^25.0.3",
        "jsdom": "^27.3.0",
        "postcss": "^8.5.6",
        "solid-icons": "^1.1.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "tailwindcss-animate": "^1.0.7",
        "tslib": "^2.8.1",
        "tsup": "^8.5.1",
        "tsup-preset-solid": "^2.2.0",
        "typescript": "~5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-solid": "^2.11.10",
        "vitest": "^4.0.16"
    },
    "typesVersions": {},
    "engines": {
        "node": ">=20",
        "pnpm": ">=10.0.0"
    },
    "module": "./dist/index.js",
    "browser": {}
}
