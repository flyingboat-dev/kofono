{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "cache": false,
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"]
        },
        "build:watch": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"]
        },
        "format": {
            "dependsOn": ["^format"]
        },
        "lint": {
            "dependsOn": ["^lint"],
            "cache": false
        },
        "lint:fix": {
            "dependsOn": ["^lint:fix"],
            "cache": false
        },
        "test": {
            "cache": false,
            "dependsOn": ["^test"]
        },
        "dev": {
            "dependsOn": ["^build"],
            "cache": false,
            "persistent": true
        },
        "doc": {
            "dependsOn": ["^build", "^build:editor"],
            "cache": false,
            "persistent": true
        },
        "build:editor": {
            "cache": false
        },
        "dev:form-store": {},
        "clean": {
            "cache": false
        }
    }
}
